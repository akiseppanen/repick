{
  "name": "@repick/vue",
  "version": "1.0.0-alpha.0",
  "main": "dist/index.js",
  "module": "dist/repick-vue.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean": "rm -rf node_modules .rts2_cache_cjs .rts2_cache_esm dist;",
    "test": "jest --env=jsdom",
    "prepublish": "npm run build",
    "watch": "rollup -w -c rollup.config.js",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@repick/core": "^1.2.1",
    "date-fns": "^2.9.0"
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-storysource": "^6.4.18",
    "@storybook/vue": "^6.4.18",
    "@storybook/vue3": "^6.4.18",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/vue": "^6.5.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.3",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/test-utils": "^2.0.0-rc.17",
    "babel-jest": "^28.1.1",
    "classnames": "^2.3.1",
    "jest": "^27.5.1",
    "jest-date-serializer": "^1.0.4",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vue": "^3.2.30",
    "vue-loader": "^17.0.0"
  },
  "jest": {
    "transform": {
      ".ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/**/*.(spec|test).{ts,tsx,js,jsx}"
    ],
    "globalSetup": "./test/setup.ts",
    "snapshotSerializers": [
      "jest-date-serializer"
    ]
  }
}
