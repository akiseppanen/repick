{
  "name": "@repick/react",
  "version": "1.2.1",
  "main": "dist/index.js",
  "module": "dist/repick-react.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean": "rm -rf node_modules .rts2_cache_cjs .rts2_cache_esm dist;",
    "test": "jest --env=jsdom",
    "prepublish": "npm run build",
    "watch": "rollup -w -c rollup.config.js",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@repick/core": "^1.2.1",
    "date-fns": "^2.9.0"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-storysource": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.3",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.0",
    "babel-jest": "^28.0.1",
    "classnames": "^2.3.1",
    "jest": "^27.5.1",
    "jest-date-serializer": "^1.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rollup": "^2.67.2",
    "rollup-plugin-typescript2": "^0.32.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/**/*.(spec|test).{ts,tsx,js,jsx}"
    ],
    "globalSetup": "./test/setup.ts",
    "snapshotSerializers": [
      "jest-date-serializer"
    ]
  }
}
