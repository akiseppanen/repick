{
  "name": "repick-react",
  "version": "0.0.2",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest --env=jsdom",
    "watch": "tsc --watch",
    "prepublish": "npm build"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "react": "^16.8.6",
    "repick-core": "^0.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.15",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "react-dom": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "ts-jest": "^24.0.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
