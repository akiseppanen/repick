{
  "name": "repick-core",
  "version": "0.0.3",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest --env=jsdom",
    "prepublish": "npm build"
  },
  "dependencies": {
    "date-fns": "^1.29.0"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "lerna": "^3.14.1",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
